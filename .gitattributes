# Track large GeoJSON files (>50MB) with Git LFS
# Small files will be in the repo and accessible via raw.githubusercontent.com URLs
# Large files need to be accessed via GitHub Releases or API

# Files over 100MB (must use LFS)
geojson-data/AU.geojson filter=lfs diff=lfs merge=lfs -text
geojson-data/CA.geojson filter=lfs diff=lfs merge=lfs -text
geojson-data/CL.geojson filter=lfs diff=lfs merge=lfs -text
geojson-data/FR.geojson filter=lfs diff=lfs merge=lfs -text
geojson-data/IE.geojson filter=lfs diff=lfs merge=lfs -text
geojson-data/JP.geojson filter=lfs diff=lfs merge=lfs -text
geojson-data/NO.geojson filter=lfs diff=lfs merge=lfs -text
geojson-data/NZ.geojson filter=lfs diff=lfs merge=lfs -text
geojson-data/PH.geojson filter=lfs diff=lfs merge=lfs -text
geojson-data/RU.geojson filter=lfs diff=lfs merge=lfs -text

# Files 50-100MB (recommended to use LFS)
geojson-data/ES.geojson filter=lfs diff=lfs merge=lfs -text
geojson-data/IN.geojson filter=lfs diff=lfs merge=lfs -text
geojson-data/MX.geojson filter=lfs diff=lfs merge=lfs -text
geojson-data/PT.geojson filter=lfs diff=lfs merge=lfs -text
geojson-data/RO.geojson filter=lfs diff=lfs merge=lfs -text
geojson-data/SE.geojson filter=lfs diff=lfs merge=lfs -text